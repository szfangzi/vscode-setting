// 将键绑定放入此文件中以覆盖默认值
[
  { "key": "alt+d", "command": "editor.action.deleteLines", "when": "editorTextFocus && !editorReadonly" },
  { "key": "ctrl+down", "command": "editor.action.moveLinesDownAction", "when": "editorTextFocus && !editorReadonly" },
  { "key": "ctrl+up", "command": "editor.action.moveLinesUpAction", "when": "editorTextFocus && !editorReadonly" },
  { "key": "ctrl+d", "command": "editor.action.copyLinesDownAction", "when": "editorTextFocus && !editorReadonly" },
  { "key": "ctrl+shift+h", "command": "editor.action.formatDocument", "when": "editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly" },
  { "key": "alt+f3", "command": "editor.action.changeAll", "when": "editorTextFocus && !editorReadonly" }               
]